{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/logs/log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "JWT": {
    "Key": "Very Secret Key Be Aware !!!",
    "Issuer": "http://localhost:5237",
    "Audience": "http://localhost:5237"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MSSQLConnection": "server = MXECMASHINA; database = MyProjDb; integrated security = true; TrustServerCertificate  = true",
    "SQLiteConnection": "Data Source =C:/Users/lukak/OneDrive/Desktop/TestDB/MyProjDb14.db"
  }
}